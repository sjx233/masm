{
  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "launch",
      "name": "Run Tests",
      "skipFiles": [
        "<node_internals>/**"
      ],
      "program": "${workspaceFolder}/lib/test/test.js",
      "envFile": "${workspaceFolder}/.env",
      "preLaunchTask": "npm: build",
      "outFiles": [
        "${workspaceFolder}/lib/**/*.js"
      ]
    }
  ]
}
